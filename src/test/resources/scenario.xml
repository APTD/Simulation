<?xml version="1.0" encoding="UTF-8"?>
<asimov:asimov
        xmlns:rail = "http://www.railml.org/schemas/2016"
        xmlns:asimov = "https://raw.githubusercontent.com/APTD/Simulation/master/src/main/xsd"
        xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation = "https://raw.githubusercontent.com/APTD/Simulation/master/src/main/xsd ../../main/xsd/asimov.xsd">
    <asimov:ai>
        <asimov:agents>
            <asimov:properties>
                <asimov:motivation>
                    <asimov:item id="minimalchanges"/>
                </asimov:motivation>
                <asimov:preference>
                    <asimov:item id="coffee"/>
                    <asimov:item id="newspaper"/>
                </asimov:preference>
            </asimov:properties>
            <asimov:instance>
                <asimov:agent id="oldpassenger">
                    <asimov:configuration>
                        <asimov:motivation id="minimalchanges" distribution="normal" minimum = "0" maximum = "10" moment1 = "5" moment2 = "2" />
                        <asimov:preference id="coffee" distribution="uniform" minimum = "0" maximum = "1" moment1 = "0.3" moment2 = "0.1"/>
                        <asimov:preference id="newspaper" distribution="normal" minimum = "0" maximum = "1" moment1 = "0.7" moment2 = "0.2"/>
                        <asimov:asl>
                            !main.
                            
                            +!main &lt;-
                            generic/print(&quot;hello world&quot;)
                            .
                        </asimov:asl>
                    </asimov:configuration>
                </asimov:agent>
            </asimov:instance>
        </asimov:agents>
    </asimov:ai>
    <asimov:network>
        <rail:infrastructure id="ID000">
        </rail:infrastructure>
        <rail:rollingstock id="ID001">
        </rail:rollingstock>
        <asimov:lineplan period="P1Y2M3DT1H10M0S">
        </asimov:lineplan>
        <rail:timetable id="ID002">
        </rail:timetable>
        <asimov:demand id="ID003">
            <asimov:odMatrix>
                <asimov:odEntry id="ID005" numPassengers="0">
                    <asimov:startNode id="ID006" pos="0.000000">
                        <rail:macroscopicNode ocpRef="ID006"/>
                    </asimov:startNode>
                    <asimov:endNode id="ID007" pos="0.000000">
                        <rail:macroscopicNode ocpRef="ID004"/>
                    </asimov:endNode>
                    <asimov:entryTimeDistribution id="ID008">
                    </asimov:entryTimeDistribution>
                    <asimov:personaDistribution id="ID009">
                    </asimov:personaDistribution>
                    <asimov:groupDistribution id="ID010">
                    </asimov:groupDistribution>
                </asimov:odEntry>
                <asimov:odEntry id="ID012" numPassengers="0">
                    <asimov:startNode id="ID013" pos="0.000000">
                        <rail:openEnd id="ID014">
                        </rail:openEnd>
                    </asimov:startNode>
                    <asimov:endNode id="ID015" pos="0.000000">
                        <rail:openEnd id="ID016">
                        </rail:openEnd>
                    </asimov:endNode>
                    <asimov:entryTimeDistribution id="ID017">
                    </asimov:entryTimeDistribution>
                    <asimov:personaDistribution id="ID018">
                    </asimov:personaDistribution>
                    <asimov:groupDistribution id="ID019">
                    </asimov:groupDistribution>
                </asimov:odEntry>
                <asimov:operatingPeriod id="ID020">
                </asimov:operatingPeriod>
            </asimov:odMatrix>
            <asimov:personas>
            </asimov:personas>
        </asimov:demand>
        <asimov:disposition id="ID021">
            <asimov:dispositionAlgorithm id="ID022">
            </asimov:dispositionAlgorithm>
            <asimov:dispositionAlgorithm id="ID023">
            </asimov:dispositionAlgorithm>
            <asimov:dispositionManager dispositionAlgorithmRef="ID017" ocpGroupRef="ID007" id="ID024">
            </asimov:dispositionManager>
            <asimov:dispositionManager dispositionAlgorithmRef="ID010" ocpGroupRef="ID015" id="ID025">
            </asimov:dispositionManager>
        </asimov:disposition>
    </asimov:network>
</asimov:asimov>
